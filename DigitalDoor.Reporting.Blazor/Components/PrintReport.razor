@if(ReportModel is not null && ShowButton == true)
{
    <div @onclick=GeneratePdf @attributes="AdditionalAttributes">
        @if(ChildContent is not null)
        {
            @ChildContent
        }
        else
        {
            <span class="button is-link btn btn-primary">Generate PDF</span>
        }
    </div>

    <ReportView ReportModel=ReportModel ShowPreview=false WrapperId="@WrapperId" />
}




